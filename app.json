{
    "expo": {
        "name": "ieatta",
        "slug": "ieatta",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.ieatta.track",
            "googleServicesFile": "./ios/ieatta/GoogleService-Info.plist"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.ieatta.track",
            "googleServicesFile": "./android/app/google-services.json"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": ["@react-native-firebase/app", "@react-native-firebase/perf", "expo-build-properties", "react-native-vision-camera"],
        "extra": {
            "eas": {
                "projectId": "d089da50-1783-11e8-b0a2-a58f1992427f"
            }
        }
    }
}
